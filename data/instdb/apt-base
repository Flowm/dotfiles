export DEBIAN_FRONTEND=noninteractive
apt update

apt install -y \
    bash-completion \
    bind9-host \
    build-essential \
    curl \
    dnsutils \
    etckeeper \
    git \
    htop \
    iotop \
    lsof \
    man-db \
    mosh \
    nmap \
    openssh-client \
    openssh-server \
    passwdqc \
    silversearcher-ag \
    silversearcher-ag \
    slurm \
    tcpdump \
    telnet \
    tmux \
    unp \
    unzip \
    vim \
    wget \
    whois \
    zsh \
;
